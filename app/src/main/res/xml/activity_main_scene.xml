<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/thermalConductivityLiquids"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="0dp"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:alpha="0" />
        <Constraint
            android:id="@+id/exp2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toBottomOf="@id/thermalConductivityLiquids"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="0dp"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:alpha="0" />
        <Constraint
            android:id="@+id/exp3"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toBottomOf="@id/exp2"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="0dp"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:alpha="0" />
        <Constraint
            android:id="@+id/exp4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toBottomOf="@id/exp3"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="0dp"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:alpha="0" />
        <Constraint
            android:id="@+id/exp5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toBottomOf="@id/exp4"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="0dp"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:alpha="0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/thermalConductivityLiquids"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/exp2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toBottomOf="@id/thermalConductivityLiquids"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/exp3"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toBottomOf="@id/exp2"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/exp4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toBottomOf="@id/exp3"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/exp5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toBottomOf="@id/exp4"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" />
    </ConstraintSet>
</MotionScene>